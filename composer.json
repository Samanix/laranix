{
  "name": "samanix/laranix",
  "description": "Laravel extended functionality",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Sam Newsome",
      "email": "samanixcom@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": "5.4.*",
    "guzzlehttp/guzzle": "6.3.*",
    "andreasindal/laravel-markdown": "1.1.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7",
    "barryvdh/laravel-debugbar": "dev-master",
    "barryvdh/laravel-ide-helper": "dev-master",
    "laravel/dusk": "1.1.*"
  },
  "autoload": {
    "psr-4": {
      "Laranix\\": "src/"
    },
    "files": [
      "src/Networker/helpers.php",
      "src/AppSettings/helpers.php",
      "src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  }
}
